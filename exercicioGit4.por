programa {
  funcao inicio() {
    inteiro numero1,numero2,numero3,numero4,p1,p2,diferenca

    escreva("Informe numero1: ")
    leia(numero1)

    escreva("Informe numero2: ")
    leia(numero2)

    escreva("Informe numero3: ")
    leia(numero3)

    escreva("Informe numero4: ")
    leia(numero4)

    p1 = (numero1 * numero2)
    p2 = (numero3 * numero4)

    diferenca = (p1 - p2)

    escreva("diferença : ", diferenca)
  }
}
