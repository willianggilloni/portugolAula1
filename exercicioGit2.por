programa {

  funcao inicio() {

    real nota1,nota2,nota3,nota4,mediaFinal

    escreva("Digite a nota 1: ")
    leia(nota1)

    escreva("Digite a note 2: ")
    leia(nota2)

    escreva("Digite a note 3: ")
    leia(nota3)

    escreva("Digite a note 4: ")
    leia(nota4)

    mediaFinal=(nota1 + nota2 + nota3 + nota4)/4)
    escreva("\n"," Média final é :", mediaFinal )

  }
}
